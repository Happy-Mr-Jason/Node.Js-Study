<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>EJS Example</title>
  <link rel="stylesheet" type="text/css" href="../resources/css/index.css">
</head>

<body>
  <h1><%= title %></h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Content</th>
        <th>Author</th>
      </tr>
    </thead>
    <tbody>
      <% list.forEach(function(item){ %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.content %></td>
        <td><%= item.author %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</body>

</html>